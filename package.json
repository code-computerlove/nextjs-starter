{
	"name": "nextjs-starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "run-p lint:*",
		"lint:next": "next lint",
		"lint:js": "eslint \"*/**/*.{js,jsx,ts,tsx}\" --fix",
		"lint:css": "stylelint \"./**/*.{css,scss}\" --fix",
		"lint:ts": "tsc -p tsconfig.json --noEmit",
		"format": "prettier \"*/**/*.{js,jsx,ts,tsx,json,md,css,scss}\" --write",
		"formatw": "prettier \"{,!(node_modules)/**/}*.js\"",
		"storybook": "start-storybook -s public -p 6006",
		"build-storybook": "build-storybook -o public/storybook",
		"prepare": "husky install",
		"test": "vitest",
		"new": "cross-env HYGEN_TMPLS=generators hygen new"
	},
	"dependencies": {
		"next": "13.0.2",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@axe-core/react": "^4.5.1",
		"@commitlint/cli": "^17.2.0",
		"@commitlint/config-conventional": "^17.2.0",
		"@storybook/addon-a11y": "^6.5.13",
		"@storybook/addon-actions": "^6.5.13",
		"@storybook/addon-essentials": "^6.5.13",
		"@storybook/addon-interactions": "^6.5.13",
		"@storybook/addon-links": "^6.5.13",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/builder-webpack5": "^6.5.13",
		"@storybook/manager-webpack5": "^6.5.13",
		"@storybook/react": "^6.5.13",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest-axe": "^3.5.5",
		"@types/node": "18.11.9",
		"@types/react": "18.0.25",
		"@types/react-dom": "18.0.8",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@typescript-eslint/parser": "^5.42.1",
		"@vitejs/plugin-react": "^2.2.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.26.0",
		"eslint-config-next": "^13.0.3",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.2",
		"hygen": "^6.2.11",
		"jsdom": "^20.0.2",
		"lint-staged": "^13.0.3",
		"msw": "^0.48.0",
		"npm-run-all": "^4.1.5",
		"postcss-loader": "^7.0.1",
		"postcss-preset-env": "^7.8.2",
		"postcss-safe-parser": "^4.0.2",
		"prettier": "^2.7.1",
		"storybook-addon-performance": "^0.16.1",
		"stylelint": "^14.14.1",
		"typescript": "4.8.4",
		"vitest": "^0.25.1",
		"vitest-axe": "^0.1.0"
	},
	"peerDependencies": {
		"postcss": "^8.4.18"
	},
	"msw": {
		"workerDirectory": "public"
	},
	"engines": {
		"node": "~18.12.0"
	}
}
