{
	"name": "nextjs-starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint-js": "eslint '*/**/*.{js,jsx,ts,tsx}' --fix",
		"lint-ts": "tsc -p tsconfig.json --noEmit",
